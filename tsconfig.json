{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "checkJs": true,
    "lib": [
      "dom",
      "dom.iterable",
      "ES2022",
      "ES2023",
      "ESNext"
    ],
    "noEmit": true,
    "module": "ESNext",
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "incremental": true,
    /* Path Aliases */
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "svgr.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.js",
    "next.config.mjs",
    "env.mjs",
    ".next/types/**/*.ts"
, "app/swap/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
